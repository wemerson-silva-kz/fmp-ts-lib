{
  "name": "fmp-ts-lib",
  "version": "1.0.3",
  "description": "ðŸš€ Complete TypeScript library for Financial Modeling Prep API with 100% endpoint coverage (298 endpoints), enterprise-grade features, and advanced financial analysis tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --skipLibCheck --target ES2020 --module ESNext --outDir dist --declaration --declarationMap --sourceMap src/index.ts",
    "build:clean": "rm -rf dist && npm run build",
    "dev": "bun run src/index.ts",
    "test": "bun test",
    "vitest": "bun test src/services/__tests__/*.ts",
    "test:coverage": "bun test --coverage",
    "test:real": "bun run test-real-api-fixed.ts",
    "test:auto-detect": "bun run test-auto-detect.ts",
    "lint": "tsc --noEmit --skipLibCheck"
  },
  "keywords": [
    "financial-modeling-prep",
    "fmp",
    "api",
    "typescript",
    "financial-data",
    "stock-market",
    "stocks",
    "finance",
    "trading",
    "investment",
    "crypto",
    "forex",
    "commodities",
    "etf",
    "mutual-funds",
    "financial-statements",
    "enterprise",
    "cache",
    "rate-limiting",
    "retry-logic",
    "portfolio-analysis",
    "financial-analysis"
  ],
  "author": {
    "name": "Wemerson Silva",
    "url": "https://github.com/wemerson-silva-kz/fmp-ts-lib"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wemerson-silva-kz/fmp-ts-lib.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "bugs": {
    "url": "https://github.com/wemerson-silva-kz/fmp-ts-lib/issues"
  },
  "homepage": "https://github.com/wemerson-silva-kz/fmp-ts-lib#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "README-ADVANCED.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  }
}
